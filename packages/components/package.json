{
  "name": "@chanzuckerberg/eds-components",
  "version": "0.7.0",
  "description": "React components for the education design system",
  "keywords": [
    "design system",
    "components"
  ],
  "author": "CZI <edu-frontend-infra@chanzuckerberg.com>",
  "homepage": "https://github.com/chanzuckerberg/edu-design-system/tree/main/packages/components",
  "license": "MIT",
  "main": "lib/index.js",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chanzuckerberg/edu-design-system.git"
  },
  "files": [
    "lib",
    "tailwind.config.js"
  ],
  "scripts": {
    "build": "yarn run build:clean && yarn run build:js && yarn run build:styles && yarn run build:finish",
    "build:clean": "rm -rf lib/",
    "build:finish": "rm -rf lib/storyUtils lib/**/*.spec.* lib/**/*.stories.*",
    "build:js": "tsc",
    "build:styles": "postcss \"src/**/*.css\" --dir lib/ --base src/ --verbose",
    "build:watch": "yarn run build:js --watch",
    "build:storybook": "build-storybook -o storybook-static",
    "chromatic": "chromatic",
    "deploy:docs": "storybook-to-ghpages --ci",
    "prepack": "yarn run build",
    "storybook": "start-storybook -p 6008",
    "storybook:axe": "yarn run build:storybook && yarn run storybook:axeOnly",
    "storybook:axeOnly": "axe-storybook --build-dir storybook-static",
    "start": "yarn run storybook",
    "test": "jest",
    "test:ci": "yarn run test --ci --coverage && cat ./coverage/lcov.info | codecov",
    "types": "tsc --noEmit"
  },
  "bugs": {
    "url": "https://github.com/chanzuckerberg/edu-design-system/issues"
  },
  "peerDependencies": {
    "react": ">= 16.8.0"
  },
  "dependencies": {
    "@chanzuckerberg/eds-tokens": "^0.7.0",
    "@tippyjs/react": "^4.2.5",
    "clsx": "^1.1.1",
    "react-uid": "^2.3.1"
  },
  "devDependencies": {
    "@chanzuckerberg/axe-storybook-testing": "^5.0.0",
    "@chanzuckerberg/story-utils": "^3.0.4",
    "@storybook/addon-a11y": "^6.4.17",
    "@storybook/addon-essentials": "^6.4.17",
    "@storybook/addon-interactions": "^6.4.17",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.4.17",
    "@storybook/storybook-deployer": "^2.8.10",
    "@storybook/testing-library": "^0.0.8",
    "@storybook/testing-react": "^1.2.3",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.16",
    "autoprefixer": "^10.4.2",
    "axe-core": "^4.3.5",
    "chromatic": "^6.4.3",
    "codecov": "^3.8.3",
    "flowgen": "^1.14.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.7",
    "postcss": "^8.4.6",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.6",
    "react": "^16.14.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^16.14.0",
    "storybook-css-modules-preset": "^1.1.1",
    "tailwindcss": "^3.0.23",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  }
}
