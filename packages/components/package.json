{
  "name": "@chanzuckerberg/czedi-kit-components",
  "version": "0.0.1-alpha.5",
  "description": "React components for czedi-kit",
  "keywords": [
    "design system",
    "components"
  ],
  "author": "Devin Witherspoon <dcwither@gmail.com>",
  "homepage": "https://github.com/chanzuckerberg/lp-design-system/tree/master/packages/czedi-kit-components#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "restricted",
    "directory": "dist/",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chanzuckerberg/lp-design-system.git"
  },
  "scripts": {
    "build": "npm run build:clean && npm run build:types && npm run build:js",
    "build:clean": "rm -rf dist/",
    "build:js": "babel src -d dist --extensions '.js,.jsx,.ts,.tsx' --ignore src/**/*.spec.* --ignore src/**/*.stories.*",
    "build:types": "tsc --project . --declaration --emitDeclarationOnly",
    "build:watch": "npm run build:js -- --watch",
    "build:storybook": "build-storybook",
    "deploy:docs": "storybook-to-ghpages --ci",
    "prepack": "npm run build && cp ./package.json dist/",
    "snapshot": "percy-storybook --widths=320,1280",
    "storybook": "start-storybook -p 6006",
    "start": "npm run storybook",
    "test": "jest",
    "test:ci": "npm run test -- --ci --coverage && cat ./coverage/lcov.info | codecov"
  },
  "bugs": {
    "url": "https://github.com/chanzuckerberg/lp-design-system/issues"
  },
  "peerDependencies": {
    "react": ">= 16.8.0"
  },
  "dependencies": {
    "@chanzuckerberg/czedi-kit-styles": "^0.0.1-alpha.5",
    "@chanzuckerberg/czedi-kit-tokens": "^0.0.1-alpha.5",
    "classnames": "^2.2.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/preset-typescript": "^7.12.7",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@commitlint/prompt-cli": "^8.3.5",
    "@linaria/babel": "^3.0.0-beta.0",
    "@linaria/core": "^3.0.0-beta.0",
    "@linaria/react": "^3.0.0-beta.0",
    "@linaria/shaker": "^3.0.0-beta.0",
    "@percy/storybook": "^3.3.0",
    "@storybook/addon-a11y": "^5.3.18",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-docs": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/preset-typescript": "^1.1.0",
    "@storybook/react": "^5.3.18",
    "@storybook/storybook-deployer": "^2.8.6",
    "@testing-library/react": "^9.3.0",
    "@types/jest": "^24.0.19",
    "@types/jest-axe": "^3.2.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-macros": "^3.0.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "jest": "^24.9.0",
    "jest-axe": "^3.2.0",
    "node-sass": "^4.14.1",
    "react": "^16.10.2",
    "react-docgen-typescript-loader": "^3.3.0",
    "react-dom": "^16.10.2",
    "sass-loader": "^8.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.0",
    "typescript": "^4.1.2",
    "vue": "^2.6.12"
  },
  "gitHead": "8680dac1fa3c685e2a35f80d9ceb157f5f5343bd"
}
