{
  "name": "@chanzuckerberg/czedi-kit-components",
  "version": "0.0.1-alpha.5",
  "description": "React components for czedi-kit",
  "keywords": [
    "design system",
    "components"
  ],
  "author": "Devin Witherspoon <dcwither@gmail.com>",
  "homepage": "https://github.com/chanzuckerberg/lp-design-system/tree/master/packages/czedi-kit-components#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "restricted",
    "directory": "dist/",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chanzuckerberg/lp-design-system.git"
  },
  "scripts": {
    "build": "npm run build:clean && npm run build:types && npm run build:js",
    "build:clean": "rm -rf dist/",
    "build:js": "babel src -d dist --extensions '.js,.jsx,.ts,.tsx' --ignore src/**/*.spec.ts,src/**/*.spec.tsx --ignore src/**/*.stories.ts,src/**/*.stories.tsx",
    "build:types": "tsc --project . --declaration --emitDeclarationOnly",
    "build:watch": "npm run build:js -- --watch",
    "build:storybook": "build-storybook",
    "deploy:docs": "storybook-to-ghpages --ci",
    "prepack": "npm run build && cp ./package.json dist/",
    "snapshot": "percy-storybook --widths=320,1280",
    "storybook": "start-storybook -p 6006",
    "start": "npm run storybook",
    "test": "jest",
    "test:ci": "npm run test -- --ci --coverage && cat ./coverage/lcov.info | codecov"
  },
  "bugs": {
    "url": "https://github.com/chanzuckerberg/lp-design-system/issues"
  },
  "peerDependencies": {
    "react": ">= 16.8.0"
  },
  "dependencies": {
    "@chanzuckerberg/czedi-kit-tokens": "^0.0.1-alpha.5",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@commitlint/prompt-cli": "^8.3.5",
    "@percy/storybook": "^3.3.0",
    "@storybook/addon-a11y": "^6.1.10",
    "@storybook/addon-actions": "^6.1.10",
    "@storybook/addon-docs": "^6.1.12",
    "@storybook/addon-links": "^6.1.10",
    "@storybook/addons": "^6.1.10",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.1.10",
    "@storybook/storybook-deployer": "^2.8.7",
    "@testing-library/react": "^9.5.0",
    "@types/jest": "^24.9.1",
    "@types/jest-axe": "^3.5.1",
    "@types/react": "^17.0.0",
    "@types/styled-components": "^5.1.5",
    "autoprefixer": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "jest": "^24.9.0",
    "jest-axe": "^3.5.0",
    "jest-styled-components": "^7.0.3",
    "postcss": "^8.2.1",
    "react": "^16.14.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^16.14.0",
    "sass-loader": "^8.0.2",
    "tailwindcss": "^2.0.2",
    "twin.macro": "^2.0.7",
    "typescript": "^4.1.2"
  },
  "gitHead": "8680dac1fa3c685e2a35f80d9ceb157f5f5343bd"
}
