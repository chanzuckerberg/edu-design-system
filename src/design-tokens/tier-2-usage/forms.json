{
  "eds": {
    "theme": {
      "form": {
        "input": {
          "border": {
            "width": {
              "value": "{eds.theme.border.width}"
            },
            "radius": {
              "value": "{eds.theme.border.radius}"
            }
          }
        }
      },
      "color": {
        "form": {
          "label": {
            "foreground": {
              "@": {
                "value": "{eds.theme.color.body.foreground.@}"
              },
              "inverted": {
                "value": "{eds.theme.color.body.foreground.inverted}"
              }
            }
          },
          "input": {
            "border": {
              "@": {
                "value": "{eds.theme.color.neutral.md.border.@}"
              },
              "hover": {
                "value": "{eds.theme.color.neutral.bold.border.@}"
              },
              "inverted": {
                "hover": {
                  "value": "{eds.theme.color.neutral.bold.border.inverted}"
                }
              },
              "focus": {
                "@": {
                  "value": "{eds.theme.color.neutral.bold.border.@}"
                },
                "inverted": {
                  "value": "{eds.theme.color.neutral.bold.border.inverted}"
                }
              },
              "error": {
                "value": "{eds.color.other.red.700}"
              }
            }
          }
        }
      }
    }
  }
}
