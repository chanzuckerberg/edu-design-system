              "hover": {
                "value": "var(--eds-theme-color-text-link-brand)"
              },
              "focus": {
                "value": "var(--eds-theme-color-text-neutral-default-inverse)"
              }
            },
            "text-decoration": {
              "@": {
                "value": "var(--eds-theme-color-border-link-brand)"
