{
  "name": "@chanzuckerberg/eds",
  "version": "0.20.0",
  "description": "The React-powered design system library for Chan Zuckerberg Initiative education web applications",
  "author": "CZI <edu-frontend-infra@chanzuckerberg.com>",
  "homepage": "https://github.com/chanzuckerberg/edu-design-system",
  "license": "MIT",
  "main": "lib/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chanzuckerberg/edu-design-system.git"
  },
  "bugs": {
    "url": "https://github.com/chanzuckerberg/edu-design-system/issues"
  },
  "files": [
    "/lib"
  ],
  "scripts": {
    "build": "yarn build:clean && yarn build:tokens && yarn build:js && yarn build:styles && yarn copy-to-lib",
    "build:clean": "rm -rf lib/",
    "build:css": "postcss \"src/**/*.css\" --dir lib/ --base src/ --verbose",
    "build:icons": "svg-sprite-generate -d src/icons/ -o src/icons/spritemap/spritemap.svg && yarn generate-icon-types",
    "build:js": "tsc --project tsconfig.build.json",
    "build:storybook": "build-storybook -o storybook-static",
    "build:styles": "postcss \"src/components/**/*.css\" --dir lib/ --base src/ --verbose",
    "build:tokens": "node ./style-dictionary.config.js",
    "chromatic": "chromatic",
    "commit": "./node_modules/.bin/cz",
    "copy-to-lib": "yarn copy-icons-to-lib && yarn copy-tokens-to-lib",
    "copy-icons-to-lib": "copyfiles -u 1 src/icons/spritemap/**/* lib",
    "copy-tokens-to-lib": "copyfiles -u 2 src/tokens-dist/**/* lib/tokens",
    "create-component": "plop",
    "deploy:docs": "storybook-to-ghpages --ci",
    "generateFlowTypes": "./scripts/generateFlowTypes.sh",
    "generate-icon-types": "node ./scripts/generateIconTypes.js",
    "lint": "yarn run lint:styles && yarn run lint:scripts",
    "lint:fix": "yarn run lint:styles:fix && yarn run lint:scripts:fix",
    "lint:styles": "stylelint --ignore-path .gitignore src/**/*.css",
    "lint:styles:fix": "yarn run lint:styles --fix",
    "lint:scripts": "eslint --quiet --ignore-path .gitignore --ext=js,jsx,ts,tsx .",
    "lint:scripts:fix": "yarn run lint:scripts --fix",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "release": "yarn build && yarn release:patch",
    "release:breaking": "yarn build && yarn release:minor",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:alpha": "standard-version --prerelease alpha",
    "start": "yarn build:tokens && yarn storybook",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:axe": "yarn run build:storybook && yarn run storybook:axeOnly",
    "storybook:axeOnly": "axe-storybook --build-dir storybook-static",
    "plop": "plop component",
    "plop:page": "plop page",
    "plop:recipe": "plop recipe",
    "test": "jest",
    "test:ci": "yarn run test --ci --coverage && cat ./coverage/lcov.info | codecov",
    "types": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">= 16.8.0"
  },
  "dependencies": {
    "@headlessui/react": "1.4.3",
    "@tippyjs/react": "4.2.5",
    "clsx": "^1.1.1",
    "downshift": "^6.1.7",
    "nanoid": "^3.3.1",
    "react-children-by-type": "^1.1.0",
    "react-focus-lock": "^2.8.1",
    "react-portal": "^4.2.1",
    "react-uid": "^2.3.1",
    "svg4everybody": "^2.1.9"
  },
  "entry": "lib/index.js",
  "types": "lib/index.d.ts",
  "devDependencies": {
    "@chanzuckerberg/axe-storybook-testing": "^5.0.0",
    "@chanzuckerberg/eslint-plugin-stories": "^3.0.1",
    "@chanzuckerberg/story-utils": "^3.0.4",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@commitlint/prompt-cli": "^16.2.1",
    "@geometricpanda/storybook-addon-badges": "^0.2.1",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@storybook/addon-a11y": "^6.4.22",
    "@storybook/addon-docs": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.4.22",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-storyshots": "^6.4.19",
    "@storybook/react": "^6.4.22",
    "@storybook/storybook-deployer": "^2.8.11",
    "@storybook/testing-library": "^0.0.9",
    "@storybook/testing-react": "^1.2.4",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@types/react-portal": "^4.0.4",
    "@types/svg4everybody": "^2.1.2",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "autoprefixer": "^10.4.2",
    "axe-core": "^4.3.5",
    "chromatic": "^6.5.3",
    "codecov": "^3.8.3",
    "copyfiles": "^2.4.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.5.8",
    "eslint-plugin-testing-library": "^5.0.5",
    "flowgen": "^1.14.1",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^10.5.4",
    "make-dir-cli": "^3.0.0",
    "plop": "^3.0.5",
    "postcss": "^8.4.12",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.0.2",
    "postcss-mixins": "^9.0.2",
    "postcss-nested": "^5.0.6",
    "postcss-simple-vars": "^6.0.3",
    "prettier": "^2.5.1",
    "prop-types": "^15.8.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "size-limit": "^4.12.0",
    "standard-version": "^9.3.2",
    "storybook-css-modules-preset": "^1.1.1",
    "style-dictionary": "^3.7.0",
    "stylelint": "^14.5.3",
    "stylelint-config-recommended": "^7.0.0",
    "svg-sprite-generator": "^0.0.7",
    "svg-spritemap-webpack-plugin": "^3.9.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2",
    "web-vitals": "^2.1.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.css": [
      "stylelint --fix"
    ]
  },
  "packageManager": "yarn@3.1.1",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
