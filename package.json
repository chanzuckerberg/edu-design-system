{
  "name": "@chanzuckerberg/eds",
  "version": "17.0.0",
  "description": "The React-powered design system library for Chan Zuckerberg Initiative education web applications",
  "author": "CZI <edu-frontend-infra@chanzuckerberg.com>",
  "homepage": "https://github.com/chanzuckerberg/edu-design-system",
  "license": "MIT",
  "type": "commonjs",
  "exports": {
    ".": "./lib/index.js",
    "./index.css": "./lib/index.css",
    "./tailwind.config": "./tailwind.config.ts"
  },
  "types": "lib/index.d.ts",
  "sideEffects": [
    "lib/index.css"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chanzuckerberg/edu-design-system.git"
  },
  "bugs": {
    "url": "https://github.com/chanzuckerberg/edu-design-system/issues"
  },
  "files": [
    "/bin",
    "/lib",
    "tailwind.config.*"
  ],
  "bin": {
    "eds-apply-theme": "bin/eds-apply-theme.js",
    "eds-import-from-figma-api": "bin/eds-import-from-figma-api.js",
    "eds-init-theme": "bin/eds-init.js",
    "eds-migrate": "bin/eds-migrate.js"
  },
  "scripts": {
    "build": "yarn build:clean && yarn build:tokens && yarn build:js && yarn build:bin",
    "build:bin": "tsc -p src/bin/tsconfig.json",
    "build:clean": "rm -rf lib/",
    "build:tokens": "rm -rf src/tokens-dist/ && node ./style-dictionary.config.js && node ./style-dictionary-dark.config.js && yarn prettier-tokens-dist",
    "build:js": "rollup --config",
    "build:storybook": "storybook build -o storybook-static",
    "build:styles": "postcss \"src/components/**/*.css\" --dir lib/ --base src/ --verbose",
    "chromatic": "chromatic",
    "create-component": "plop",
    "lint": "yarn run lint:styles && yarn run lint:scripts",
    "lint:fix": "yarn run lint:styles:fix && yarn run lint:scripts:fix",
    "lint:styles": "stylelint src/**/*.css .storybook/**/*.css",
    "lint:styles:fix": "yarn run lint:styles --fix",
    "lint:scripts": "eslint --ignore-path .gitignore --ext=js,jsx,ts,tsx .",
    "lint:scripts:fix": "yarn run lint:scripts --fix",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "prettier-tokens-dist": "prettier --write \"src/tokens-dist/**/*.{js,jsx,ts,tsx}\"",
    "release": "yarn build && standard-version",
    "release:major": "yarn release --release-as major",
    "release:minor": "yarn release --release-as minor",
    "release:patch": "yarn release --release-as patch",
    "release:alpha": "yarn release --prerelease alpha",
    "start": "yarn build:tokens && yarn storybook",
    "storybook": "storybook dev -p 6006",
    "plop": "plop component",
    "prepare": "husky",
    "test": "jest",
    "test:storybook": "test-storybook",
    "test:ci": "yarn run test --ci --coverage && cat ./coverage/lcov.info | codecov",
    "types": "tsc --noEmit && npm run build:bin -- --noEmit"
  },
  "peerDependencies": {
    "react": ">= 17",
    "react-dom": ">= 17"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@tanstack/react-table": "^8.21.3",
    "@tippyjs/react": "^4.2.6",
    "chalk": "^4.1.2",
    "clsx": "^2.1.1",
    "enquirer": "^2.4.1",
    "graphemer": "^1.4.0",
    "jsonfile": "^6.2.0",
    "lilconfig": "^3.1.3",
    "lodash": "^4.17.21",
    "ora": "5.4.1",
    "react-children-by-type": "^1.1.0",
    "react-uid": "^2.4.0",
    "style-dictionary": "^3.9.2",
    "ts-dedent": "^2.2.0",
    "ts-morph": "^27.0.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@chanzuckerberg/eslint-config-edu-js": "^1.1.0",
    "@chanzuckerberg/eslint-config-edu-ts": "^1.0.9",
    "@chanzuckerberg/eslint-plugin-edu-react": "^1.1.9",
    "@chanzuckerberg/eslint-plugin-stories": "^3.2.14",
    "@chanzuckerberg/story-utils": "^4.0.8",
    "@chromatic-com/storybook": "^4.1.3",
    "@codecov/rollup-plugin": "^1.9.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@omlet/cli": "^2.0.0",
    "@rollup/plugin-commonjs": "^28.0.9",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@storybook/addon-a11y": "^9.1.16",
    "@storybook/addon-docs": "^9.1.16",
    "@storybook/addon-links": "^9.1.16",
    "@storybook/addon-styling-webpack": "^2.0.0",
    "@storybook/addon-themes": "^9.1.16",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.6",
    "@storybook/react": "^9.1.16",
    "@storybook/react-webpack5": "^9.1.16",
    "@storybook/test-runner": "^0.24.0",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/babel__core": "^7",
    "@types/jest": "^29.5.14",
    "@types/jsonfile": "^6",
    "@types/lodash": "^4.17.21",
    "@types/node": "^20.19.25",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@types/yargs": "^17.0.35",
    "axe-core": "4.11.0",
    "chromatic": "^13.3.4",
    "codecov": "^3.8.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-jest": "^28.14.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-storybook": "^9.1.16",
    "eslint-plugin-testing-library": "^7.13.5",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-stylelint": "^7.3.0",
    "jsdom-testing-mocks": "^1.16.0",
    "lint-staged": "^15.5.2",
    "plop": "^4.0.4",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-import": "^16.1.1",
    "postcss-loader": "^8.2.0",
    "postcss-mixins": "^12.1.2",
    "postcss-nested": "^7.0.2",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^4.53.3",
    "rollup-plugin-postcss": "^4.0.2",
    "standard-version": "^9.5.0",
    "storybook": "^9.1.16",
    "storybook-addon-tag-badges": "^2.0.4",
    "style-dictionary": "^3.9.2",
    "stylelint": "^16.26.1",
    "stylelint-config-recommended": "^17.0.0",
    "tailwindcss": "^3.4.18",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.css": [
      "stylelint --fix --allow-empty-input"
    ]
  },
  "packageManager": "yarn@4.12.0"
}
